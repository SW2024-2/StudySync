<h2>新規登録</h2>
<%= form_for @user , url: {action: :create} do |f| %>
    <%= f.label :name, '名前' %>
    <%= f.text_field :name %>
    <%= f.label :email, 'メールアドレス' %>
    <%= f.text_field :email %>
    <%= f.label :uid, 'ユーザID' %>
    <%= f.text_field :uid %>
    <%= f.label :pass, 'パスワード' %>
    <%= f.password_field :pass %>
    <%= f.submit '登録' %>
<% end %>